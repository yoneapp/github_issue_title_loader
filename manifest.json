{
  "name": "GitHub Issue Title Loader",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://github.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://*.qiita.com/*",
        "http://qiita.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
